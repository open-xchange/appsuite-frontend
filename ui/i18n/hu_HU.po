msgid "Every %1$d month on the %2$s %3$s."
msgid_plural "Every %1$d months on the %2$s %3$s."
msgstr[0] "Minden %1$d. hónap %2$s %3$s."
msgstr[1] "Minden %1$d. hónap %2$s. %3$s."

