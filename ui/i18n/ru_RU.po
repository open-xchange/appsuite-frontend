msgstr[1] "Каждые %1$d недели по рабочим дням."
msgstr[2] "Каждые %1$d недель по рабочим дням."

#. recurrence string
#. special case: the weekly interval is 1 and Sat and Sun are checked
#: apps/io.ox/calendar/util.js module:io.ox/calendar
msgctxt "weekly"
msgid "Every weekend."
msgstr "Каждые выходные."

#. recurrence string
#. the case %1$d == 1 is handled separately and will not be used
#. %1$d: number of weeks per interval
#: apps/io.ox/calendar/util.js module:io.ox/calendar
