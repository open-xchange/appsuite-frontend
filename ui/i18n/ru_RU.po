msgstr[1] "Каждую %1$d недели по выходным."
msgstr[2] "Каждую %1$d недель по выходным."

#. recurrence string
#. special case: the weekly interval is 1
#. %1$s: day string, e.g. "Friday" or "Monday, Tuesday, Wednesday"
#. day string will be in nominative form
#: apps/io.ox/calendar/util.js module:io.ox/calendar
#, c-format
msgctxt "weekly"
msgid "Every %1$s."
msgstr "Каждые %1$s."

#. recurrence string
#. the case %1$d == 1 is handled separately and will not be used
